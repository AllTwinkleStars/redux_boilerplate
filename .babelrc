{
  	"presets": [
    	"react",
    	"es2015",
    	"stage-0"
  	],
  	"plugins": [
    	"transform-decorators-legacy",
	],
	"env": {
		"start": {
			"plugins": [
				[
					"react-transform", {
						"transforms": [
							{
								"transform": "react-transform-hmr",
								"imports": ["react"],
								"locals": ["module"]
							}
						]
					}
				]
			]
		}
	}

}